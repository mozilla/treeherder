{
    "db_control":{
        "disable_foreign_key_checks":{

            "sql":"SET FOREIGN_KEY_CHECKS = 0",

            "host_type":"master_host"
        },
        "enable_foreign_key_checks":{

            "sql":"SET FOREIGN_KEY_CHECKS = 1",

            "host_type":"master_host"

        }
    },
    "selects":{
        "get_last_insert_id":{

            "sql":"SELECT LAST_INSERT_ID() AS `id`",

            "host_type":"master_host"
        }
    }
}

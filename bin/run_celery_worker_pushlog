#!/usr/bin/env bash

# Make non-zero exit codes & other errors fatal.
set -euo pipefail

SRC_DIR=$(dirname "$(dirname "${BASH_SOURCE[0]}")")
cd "$SRC_DIR"

source vagrant/env.sh

exec newrelic-admin run-program celery -A treeherder worker \
    -Q pushlog \
    --concurrency=5 -l INFO \
    -n pushlog.%h

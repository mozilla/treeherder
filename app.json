{
  "addons": ["scheduler", "cloudamqp", "heroku-redis", "papertrail:choklad"],
  "buildpacks": [
    {
      "url": "https://buildpack-registry.s3.amazonaws.com/buildpacks/edmorley/timestamps.tgz"
    },
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "BROKER_URL": {
      "value": "$CLOUDAMQP_URL"
    },
    "PULSE_PUSH_SOURCES": {
      "value": "[{'root_url': 'https://taskcluster.net', 'github': true, 'hgmo': true, 'pulse_url': 'amqp://mozilla_treeherder_testing:mozilla123@pulse.mozilla.org:5671/?ssl=true'}]"
    },
    "PULSE_TASK_SOURCES": {
      "value": "[{'root_url': 'https://taskcluster.net', 'pulse_url': 'amqp://mozilla_treeherder_testing:mozilla123@pulse.mozilla.org:5671/?ssl=true'}]"
    },
    "ROOT_URL": {
      "value": "https://taskcluster.net"
    },
    "SITE_URL": {
      "value": "https://treeherder-prototype-pr-TBD.herokuapp.com/"
    },
    "TREEHERDER_DJANGO_SECRET_KEY": {
      "value": "secret-key-of-at-least-50-characters-to-pass-check-deploy"
    },
    "YARN_PRODUCTION": {
      "value": "true"
    }
  },
  "formation": {
    "worker_store_pulse_data": {
      "quantity": 1,
      "size": "Standard-1X"
    },
    "pulse_listener_tasks": {
      "quantity": 1,
      "size": "Standard-1X"
    },
    "celery_scheduler": {
      "quantity": 1,
      "size": "Standard-1X"
    },
    "worker_log_parser": {
      "quantity": 1,
      "size": "Standard-1X"
    },
    "worker_misc": {
      "quantity": 1,
      "size": "Standard-1X"
    },
    "pulse_listener_pushes": {
      "quantity": 1,
      "size": "Standard-1X"
    },
    "web": {
      "quantity": 1,
      "size": "Standard-1X"
    }
  },
  "name": "treeherder",
  "scripts": {},
  "stack": "heroku-18"
}

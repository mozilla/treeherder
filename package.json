{
  "name": "treeherder",
  "description": "Mozilla Treeherder reporting dashboard",
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/treeherder.git"
  },
  "license": "MPL-2.0",
  "engines": {
    "node": "7.10.0"
  },
  "dependencies": {
    "angular": "1.5.11",
    "angular-cookies": "1.5.11",
    "angular-local-storage": "0.5.2",
    "angular-resource": "1.5.11",
    "angular-route": "1.5.11",
    "angular-sanitize": "1.5.11",
    "angular-toarrayfilter": "1.0.2",
    "angular-ui-bootstrap": "1.3.3",
    "angular-ui-router": "0.4.2",
    "bootstrap": "3.3.7",
    "font-awesome": "4.7.0",
    "hawk": "6.0.1",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "2.2.4",
    "jquery.scrollto": "2.1.2",
    "js-yaml": "3.8.4",
    "json-schema-defaults": "0.3.0",
    "lodash": "4.17.4",
    "mousetrap": "1.6.1",
    "neutrino": "^5.8.0",
    "neutrino-middleware-copy": "^5.8.0",
    "neutrino-middleware-eslint": "^5.8.0",
    "neutrino-preset-react": "^5.8.0",
    "ngreact": "0.4.0",
    "raw-loader": "0.5.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "taskcluster-client": "2.3.0",
    "webpack": "^2.6.0",
    "webpack-md5-hash": "0.0.5"
  },
  "devDependencies": {
    "angular-mocks": "1.5.11",
    "enzyme": "2.7.1",
    "eslint": "3.19.0",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.6.1",
    "karma": "1.7.0",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "1.1.0",
    "neutrino-preset-karma": "^5.8.0",
    "react-addons-test-utils": "15.3.2"
  },
  "neutrino": {
    "use": [
      "./neutrino-custom/lint.js",
      "neutrino-preset-react",
      "./neutrino-custom"
    ],
    "options": {
      "source": "ui",
      "output": "dist",
      "entry": "entry-index.js",
      "tests": "tests",
      "compile": {
        "targets": {
          "browsers": [
            "last 1 Chrome versions",
            "last 1 Firefox versions",
            "last 1 Edge versions",
            "last 1 Safari versions"
          ]
        }
      }
    }
  },
  "scripts": {
    "build": "./node_modules/neutrino/bin/neutrino build",
    "lint": "NODE_ENV=development ./node_modules/eslint/bin/eslint.js --ext .js,.jsx ui/",
    "start": "./node_modules/neutrino/bin/neutrino start",
    "start:local": "LOCAL_WATCH=true ./node_modules/neutrino/bin/neutrino start",
    "start:stage": "SERVICE_DOMAIN=https://treeherder.allizom.org ./node_modules/neutrino/bin/neutrino start",
    "test": "./node_modules/neutrino/bin/neutrino test",
    "test:watch": "./node_modules/neutrino/bin/neutrino test --watch --use"
  }
}
